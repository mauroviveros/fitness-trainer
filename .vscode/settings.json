{
  "editor.formatOnSave": false,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "IDX.agenticWorkspace": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0075ee",
    "activityBar.background": "#0075ee",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ff9fce",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#0075ee",
    "statusBar.background": "#005cbb",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0075ee",
    "statusBarItem.remoteBackground": "#005cbb",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#005cbb",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#005cbb99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#005cbb",
  "conventionalCommits.scopes": [
    "auth",
    "customers",
    "shared",
    "exercises"
  ]
}
