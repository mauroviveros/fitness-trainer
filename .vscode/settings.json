{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always"
    },
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always"
    },
    "editor.formatOnSave": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#b45050",
    "activityBar.background": "#b45050",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#63bc63",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#b45050",
    "statusBar.background": "#923f3f",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#b45050",
    "statusBarItem.remoteBackground": "#923f3f",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#923f3f",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#923f3f99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#923F3F",
  "conventionalCommits.scopes": [
    "auth"
  ],
}
